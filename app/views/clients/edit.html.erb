<% provide(:title, "Edit client") %>
<h1>Update your profile</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@client) do |f| %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      

      <%= f.submit "Save changes", class: "btn mybtn btn-large btn-primary" %>
    <% end %>

    
  </div>
</div>